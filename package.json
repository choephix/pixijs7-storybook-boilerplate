{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview  --host",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@pixi/storybook-renderer": "^0.0.4",
    "@pixi/storybook-webpack5": "^0.0.4",
    "@storybook/addon-essentials": "^7.0.0-rc.4",
    "@storybook/addon-interactions": "^7.0.0-rc.4",
    "@storybook/addon-links": "^7.0.0-rc.4",
    "@storybook/blocks": "^7.0.0-rc.4",
    "@storybook/testing-library": "^0.0.14-next.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-rc.4",
    "typescript": "^4.9.5",
    "vite": "^4.2.0"
  },
  "dependencies": {
    "pixi.js": "^7.2.1"
  },
  "babel": {
    "sourceType": "unambiguous",
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "chrome": 100
          }
        }
      ],
      "@babel/preset-typescript"
    ],
    "plugins": []
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "quoteProps": "consistent",
    "singleQuote": false,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "trailingComma": "es5"
  }
}
